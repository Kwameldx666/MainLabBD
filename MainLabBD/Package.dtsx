          SQLTask:SqlStatementSource="-- PersonalInfoEmployees&#xA;IF OBJECT_ID('dbo.PersonalInfoEmployees', 'U') IS NULL&#xA;BEGIN&#xA;    CREATE TABLE dbo.PersonalInfoEmployees (&#xA;        EmployeeID INT IDENTITY(1,1) PRIMARY KEY,&#xA;        FirstName NVARCHAR(50) NOT NULL,&#xA;        LastName NVARCHAR(50) NOT NULL,&#xA;        UserName NVARCHAR(50) NOT NULL,&#xA;        [Password] NVARCHAR(255) NOT NULL&#xA;    );&#xA;    PRINT 'Таблица PersonalInfoEmployees создана.';&#xA;END&#xA;&#xA;-- Departments&#xA;IF OBJECT_ID('dbo.Departments', 'U') IS NULL&#xA;BEGIN&#xA;    CREATE TABLE dbo.Departments (&#xA;        DepartmentID INT IDENTITY(1,1) PRIMARY KEY,&#xA;        DepartmentName VARCHAR(100),&#xA;        EmployeeCount INT,&#xA;        DirectorID INT,&#xA;        CONSTRAINT FK_Departments_Director FOREIGN KEY (DirectorID) REFERENCES dbo.PersonalInfoEmployees(EmployeeID)&#xA;    );&#xA;    PRINT 'Таблица Departments создана.';&#xA;END&#xA;&#xA;-- Projects&#xA;IF OBJECT_ID('dbo.Projects', 'U') IS NULL&#xA;BEGIN&#xA;    CREATE TABLE dbo.Projects (&#xA;        ProjectID INT IDENTITY(1,1) PRIMARY KEY,&#xA;        ProjectName NVARCHAR(100) NOT NULL,&#xA;        Difficulty NVARCHAR(50),&#xA;        ManagerID INT,&#xA;        CONSTRAINT FK_Projects_Manager FOREIGN KEY (ManagerID) REFERENCES dbo.PersonalInfoEmployees(EmployeeID)&#xA;    );&#xA;    PRINT 'Таблица Projects создана.';&#xA;END&#xA;&#xA;-- Tasks&#xA;IF OBJECT_ID('dbo.Tasks', 'U') IS NULL&#xA;BEGIN&#xA;    CREATE TABLE dbo.Tasks (&#xA;        TaskID INT IDENTITY(1,1) PRIMARY KEY,&#xA;        TaskName VARCHAR(MAX) NOT NULL,&#xA;        ProjectID INT NOT NULL,&#xA;        Status VARCHAR(50),&#xA;        Difficulty VARCHAR(50),&#xA;        CONSTRAINT FK_Tasks_Projects FOREIGN KEY (ProjectID) REFERENCES dbo.Projects(ProjectID)&#xA;    );&#xA;    PRINT 'Таблица Tasks создана.';&#xA;END&#xA;&#xA;-- AnnualReviews&#xA;IF OBJECT_ID('dbo.AnnualReviews', 'U') IS NULL&#xA;BEGIN&#xA;    CREATE TABLE dbo.AnnualReviews (&#xA;        ReviewID INT IDENTITY(1,1) PRIMARY KEY,&#xA;        EmployeeID INT NOT NULL,&#xA;        FinalScore INT,&#xA;        CONSTRAINT FK_AnnualReviews_Employees FOREIGN KEY (EmployeeID) REFERENCES dbo.PersonalInfoEmployees(EmployeeID)&#xA;    );&#xA;    PRINT 'Таблица AnnualReviews создана.';&#xA;END&#xA;&#xA;&#xA;IF NOT EXISTS (SELECT * FROM sys.tables WHERE name = 'ProfessionalInfoEmployees' AND schema_id = SCHEMA_ID('dbo'))&#xA;BEGIN&#xA;    CREATE TABLE dbo.ProfessionalInfoEmployees (&#xA;        ProfessionalInfoEmployeesID INT IDENTITY(1,1) PRIMARY KEY,&#xA;        EmployeeID INT NOT NULL,&#xA;        DepartmentID INT NOT NULL,&#xA;        ProjectID INT,&#xA;        TaskID INT,&#xA;        HireDate DATE,&#xA;        YearsOfExperience INT,&#xA;        &#xA;        CONSTRAINT FK_ProfessionalInfoEmployees_Employee&#xA;            FOREIGN KEY (EmployeeID) REFERENCES dbo.PersonalInfoEmployees(EmployeeID),&#xA;        &#xA;        CONSTRAINT FK_ProfessionalInfoEmployees_Department&#xA;            FOREIGN KEY (DepartmentID) REFERENCES dbo.Departments(DepartmentID),&#xA;        &#xA;        CONSTRAINT FK_ProfessionalInfoEmployees_Project&#xA;            FOREIGN KEY (ProjectID) REFERENCES dbo.Projects(ProjectID),&#xA;        &#xA;        CONSTRAINT FK_ProfessionalInfoEmployees_Task&#xA;            FOREIGN KEY (TaskID) REFERENCES dbo.Tasks(TaskID)&#xA;    );&#xA;END&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
