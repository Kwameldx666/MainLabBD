uct успешно создана.';&#xA;END&#xA;ELSE&#xA;BEGIN&#xA;    PRINT 'Таблица Product уже существует.';&#xA;END&#xA;&#xA;-- Проверка и создание таблицы ProductItemdDb&#xA;IF NOT EXISTS (SELECT * FROM INFORMATION_SCHEMA.TABLES WHERE TABLE_NAME = 'ProductItemdDb')&#xA;BEGIN&#xA;    CREATE TABLE [ProductItemdDb] (&#xA;        [order_item_id] int,&#xA;        [order_id] int,&#xA;        [product_id] int,&#xA;        [quantity] int,&#xA;        [unit_price] varchar(255),&#xA;        [discount] int&#xA;    );&#xA;    PRINT 'Таблица ProductItemdDb успешно создана.';&#xA;END&#xA;ELSE&#xA;BEGIN&#xA;    PRINT 'Таблица ProductItemdDb уже существует.';&#xA;END&#xA;&#xA;PRINT 'Выполнение скрипта завершено.';" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
