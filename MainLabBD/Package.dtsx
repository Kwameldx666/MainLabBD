          SQLTask:SqlStatementSource="IF OBJECT_ID('dbo.PersonalInfoEmployees', 'U') IS NULL&#xA;BEGIN&#xA;    CREATE TABLE dbo.PersonalInfoEmployees (&#xA;        EmployeeID INT IDENTITY(1,1) PRIMARY KEY,&#xA;        FirstName NVARCHAR(50) NOT NULL,&#xA;        LastName NVARCHAR(50) NOT NULL,&#xA;        UserName NVARCHAR(50) NOT NULL,&#xA;        [Password] NVARCHAR(255) NOT NULL&#xA;    );&#xA;    PRINT 'Таблица PersonalInfoEmployees создана.';&#xA;END&#xA;ELSE&#xA;BEGIN&#xA;    PRINT 'Таблица PersonalInfoEmployees уже существует.';&#xA;END&#xA;&#xA;IF OBJECT_ID('dbo.Departments', 'U') IS NULL&#xA;BEGIN&#xA;    CREATE TABLE dbo.Departments (&#xA;        DepartmentID INT IDENTITY(1,1) PRIMARY KEY,&#xA;        DepartmentName VARCHAR(100),&#xA;        EmployeeCount INT,&#xA;        DirectorID INT&#xA;    );&#xA;    PRINT 'Таблица Departments создана.';&#xA;END&#xA;ELSE&#xA;BEGIN&#xA;    PRINT 'Таблица Departments уже существует.';&#xA;END&#xA;&#xA;IF OBJECT_ID('dbo.Projects', 'U') IS NULL&#xA;BEGIN&#xA;    CREATE TABLE dbo.Projects (&#xA;        ProjectID INT IDENTITY(1,1) PRIMARY KEY,&#xA;        ProjectName NVARCHAR(100) NOT NULL,&#xA;        Difficulty NVARCHAR(50),&#xA;        ManagerID INT&#xA;    );&#xA;    PRINT 'Таблица Projects создана.';&#xA;END&#xA;ELSE&#xA;BEGIN&#xA;    PRINT 'Таблица Projects уже существует.';&#xA;END&#xA;&#xA;&#xA;IF OBJECT_ID('dbo.Tasks', 'U') IS NULL&#xA;BEGIN&#xA;    CREATE TABLE dbo.Tasks (&#xA;        TaskID INT IDENTITY(1,1) PRIMARY KEY,&#xA;        TaskName VARCHAR(MAX) NOT NULL,&#xA;        ProjectID INT,&#xA;        Status VARCHAR(50),&#xA;        Difficulty VARCHAR(50)&#xA;    );&#xA;    PRINT 'Таблица Tasks создана.';&#xA;END&#xA;ELSE&#xA;BEGIN&#xA;    PRINT 'Таблица Tasks уже существует.';&#xA;END&#xA;&#xA;&#xA;IF OBJECT_ID('dbo.AnnualReviews', 'U') IS NULL&#xA;BEGIN&#xA;    CREATE TABLE dbo.AnnualReviews (&#xA;        ReviewID INT IDENTITY(1,1) PRIMARY KEY,&#xA;        EmployeeID INT NOT NULL,&#xA;        FinalScore INT,&#xA;        CONSTRAINT FK_AnnualReviews_Employees FOREIGN KEY (EmployeeID) REFERENCES Employees(EmployeeID)&#xA;    );&#xA;    PRINT 'Таблица AnnualReviews создана.';&#xA;END&#xA;ELSE&#xA;BEGIN&#xA;    PRINT 'Таблица AnnualReviews уже существует.';&#xA;END" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
