        string jsonFileContent = File.ReadAllText(@"C:\Users\Kwameldx666\Desktop\Data SourceBd\Departments.json");

        // Deserialize JSON using JavaScriptSerializer (or use Newtonsoft.Json or System.Text.Json)
        JavaScriptSerializer js = new JavaScriptSerializer();
        List<Departaments> departaments = js.Deserialize<List<Departaments>>(jsonFileContent);

        // Iterate through each departament object and add rows to Output0Buffer
